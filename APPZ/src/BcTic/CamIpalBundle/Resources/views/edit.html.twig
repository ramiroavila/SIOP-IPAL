{% extends '::base.html.twig' %}

{% block breadcrumb %}
<ol class="breadcrumb">
  <li><a href="{{ path('welcome') }}">{{ 'Inicio' | trans }}</a></li>
  <li class="active">{{ 'Mantenedor' | trans }}</li>
  <li>{% block breadcrumb_list %}{% endblock %}</li>
  <li class="active">{{ title }}</li>
</ol>
{% endblock %}

{% block body -%}
    <h3>{{ 'Editar' | trans }} {{ title }} </h3>

    {{ form(edit_form) }}

    <p class="text-left">
    <a href="{{ back_path }}">
      <button type="button" class="btn btn-default btn-xs">
        <span class="glyphicon glyphicon-th-list"></span> {{ 'Volver' | trans }}  
      </button>
    </a>
    </p>      


{% endblock %}
